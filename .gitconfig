[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  pr = pull --rebase
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  ds = diff --staged
[core]
  editor = vim
  pager = less -F -X
[push]
  default = simple
